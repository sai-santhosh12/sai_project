 enum nad_reset_reason_t {
	DTC_NAD_RESET_REASON_CLEAR	= 0, 	// When DTC is clear, the value is set
	DTC_REQUESTED_RESET			= 1,	// When DTC is set, NAD reset is occurred by requested reset (ex. another service)
	DTC_REQUESTED_SHUTDOWN		= 2,	// When DTC is set, NAD shutdown is occurred by requested shutdown (ex. another service)
	DTC_UNEXPECTED_RESET		= 3,	// When DTC is set, NAD reset is occurred unexpectedly
	DTC_HAPPEN_ERRORFATAL		= 4		// When DTC is set, NAD ERR Fatal is occurred
};
 
