 enum exception_level_t {
	LEVEL_0_NOMAL			= 0,
	LEVEL_1_MODEM_RESET 	= 1,
	LEVEL_2_SYSTEM_RESET 	= 2,
	LEVEL_3_SYSTEM_SHUTDOWN	= 3,
	LEVEL_MAX,
};

enum exception_reason_t {
	REASON_BY_MIN			= 0,
	REASON_BY_NAD			= 1,
	REASON_BY_M4			= 2,
	REASON_BY_NO_NAD_COMM	= 3,
	REASON_BY_A35			= 4,
	REASON_BY_REQ_SHUTDOWN	= 5,
	REASON_MAX,
};
 
 
