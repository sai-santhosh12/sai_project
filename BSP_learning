568       * @brief Radio ready duration timer value in milli seconds
569       */
570      static const uint32_t RADIO_READY_DURATION_MS = 40000U; /** 40 sec */
571      /**
572       * @brief Request poll duration timer value in milli seconds
573       */
574      static const uint32_t REQUEST_POLL_DURATION_MS = 5000U; /** 5 sec */
575      /**
576       * @brief Response poll duration timer value in milli seconds
577       */
578      static const uint32_t RESPONSE_POLL_DURATION_MS = 5000U; /** 5 sec */


