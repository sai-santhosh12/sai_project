initVifManager and onVifReceive these two methods will update pwf state and abstractPWFState will only updated on these cases only

case PWF_STATE_INVALID :
         {
              abstractPWFState =  PowerModeConst::ABSTRACT_PARKING;
              break;
          }
          case PWF_STATE_LIVING :
          case PWF_STATE_READINESS_TO_DRIVE_STOP :
          {
              abstractPWFState = PowerModeConst::ABSTRACT_LIVING;
              break;
          }
          case PWF_STATE_READINESS_TO_DRIVE_INIT :
          case PWF_STATE_CHECK_ANALYS_DIAGNOSTICS :
          case PWF_STATE_DRIVE :
          {
              abstractPWFState = PowerModeConst::ABSTRACT_DRIVING;
              break;
          }
          default :
              abstractPWFState = PowerModeConst::ABSTRACT_UNKNOWN;
			  
			  
